sub trim_outbuffer {
    if ( $show_extract || $full_diffs ) { return ; }
    if ( $out_buffer =~ "(.*\n.*\n.*\n.*\n.*\n.*\n.*\n.*\n.*\n.*\n.*\n.*\n).*\n.*\n" ) {
  $out_buffer = "$1" . "\t\t...\n" ;
    }
}
