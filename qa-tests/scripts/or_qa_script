        if ( ! -d $pararundir ) {
            mkdir $pararundir,0777;
        }
    }
    $orgarg = " ";
    $tmp = 1;
    while ( $ARGV[$tmp] ) {
        $a = $ARGV[$tmp] ;
  if ( $a =~ /^--paraqa(=)?(.*)/ ) {
        }else{
            $orgarg .= " ".$ARGV[$tmp] ;
        }
        $tmp++;
    }

    print "Running $paraqa Parallel QA in $paradir\n";

    $num_runs_attempted  = 0;
    $num_runs_good  = 0;
