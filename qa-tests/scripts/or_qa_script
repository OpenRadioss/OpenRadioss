    unlink "brief.info" ;

    unlink <*~> ;

    system ("rm -rf build-[0-9][0-9]*"); # build subdirs
    system ("rm -rf ../Scratch_*");       # parallel run dirs

    open ( RUNID, "> $run_id_file" );
    print  RUNID "done\n" ;
    close  RUNID ;

}
