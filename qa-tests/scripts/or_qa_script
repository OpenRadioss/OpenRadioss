sub check_new_files {

    my @filelist;
    my $k = 0;
    ($dev,$ino,$mode,$nlink,$uid,$gid,$rdev,$size,
     $atime,$mstarttime,$ctime,$blksize,$blocks)
  = stat("$_[0]");
#    print "$_[0]=".join(',',($dev,$ino,$mode,$nlink,$uid,$gid,$rdev,$size,
