sub cleanall {
    
    $screen_output_dir  = "Screen_Output" ;
    $extracted_data_dir = "Extracted_data" ;
    $results_data_dir   = "Results_data" ;
    $other_output_dir   = "Other_Output" ;
    $qa_summary         = "QA.summary" ;
    $local_executable   = "QA.code*" ;  # can be .exe on Windoze

    &cleanup ;
    
